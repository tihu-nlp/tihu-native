{
  "name": "tihu-native-grpc",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "start:dist": "node dist/server/index.js",
    "dev": "nodemon --inspect ./src/server/index.ts",
