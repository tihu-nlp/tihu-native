    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "google-protobuf": "^3.11.4",
    "grpc": "^1.24.2",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0"
  },
  "optionalDependencies": {
    "grpc-tools": "^1.8.1",
    "grpc_tools_node_protoc_ts": "^3.0.0",
    "protoc-gen-grpc-web": "^1.0.5"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.2",
    "@types/jest": "^25.2.1",
    "@types/pino": "^6.0.0",
    "grpc-web": "^1.0.7",
    "jest": "^25.5.4",
    "nodemon": "^2.0.3",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "xhr2": "^0.2.0"
