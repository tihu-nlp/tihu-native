{
  "name": "tihu-native",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "lerna exec --scope tihu-native-app -- yarn start",
    "start:mock": "lerna exec -- yarn start",
    "mock-api": "lerna exec --scope tihu-grpc-js -- yarn start",
    "postinstall": "lerna exec -- yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pretty-quick --write"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lerna": "^3.16.4",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  }
}
